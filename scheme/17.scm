(define (w n)
    (cond ((= n 0) 0)
          ((= n 1) 3)
          ((= n 2) 3)
          ((= n 3) 5)
          ((= n 4) 4)
          ((= n 5) 4)
          ((= n 6) 3)
          ((= n 7) 5)
          ((= n 8) 5)
          ((= n 9) 4)
          ((= n 10) 3)
          ((= n 11) 6)
          ((= n 12) 6)
          ((= n 13) 8)
          ((= n 14) 8)
          ((= n 15) 7)
          ((= n 16) 7)
          ((= n 17) 9)
          ((= n 18) 8)
          ((= n 19) 8)
          ((= n 20) 6)
          ((= n 30) 6)
          ((= n 40) 5)
          ((= n 50) 5)
          ((= n 60) 5)
          ((= n 70) 7)
          ((= n 80) 6)
          ((= n 90) 6)
          ((< n 100) (+ (w (- n (remainder n 10))) (w (remainder n 10))))
          ((< n 1000) 
            (if (= 0 (remainder n 100))
              (+ (w (/ (- n (remainder n 100)) 100)) 7)
              (+ (w (/ (- n (remainder n 100)) 100)) 10 (w (remainder n 100)))))
          ((= n 1000) 11)
          (else 0)))
(define (r l h) ;range
    (if (> l h) '()
        (cons l (r (+ l 1) h))))
(define (solve)
    (reduce + '() (map w (r 1 1000))))
